<!DOCTYPE html>
<html>
<head>
<title> <%= title %> </title>
<style>
.headline {
    color:  #0903ff;
    font-size: 30px;
}
.back {
    color:  #0903ff;
    font-size: 14px;
}

.submit {
  background-color: #0903ff;
  color: white;
  padding: 16px 5px;
  margin: 8px 10px;
  border: none;
  cursor: pointer;
  width: 10%;
  opacity: 1;
}

</style>
</head>
<body>
<p class=headline><%= usersData.firstName %></p>

<table border =1>
        <thead>
            <td>Alcohol Name</td>
            <td>Score</td>
            <td>Score By Letter</td>
        </thead>
        <tbody>
            <% var i = 0 %>
            <% for(let alcoholList of alcoholsData) { %>
                <tr>
                    <td><%= alcoholList.dataValues.alcohol_name%></td>
                    <td>
                        <!-- kerja lagi dari sini -->
                        <% for(let subjectList of studentList.dataValues.Subjects) { %>
                            <% if(subjectList.SubjectStudent.dataValues.score == undefined) { %>
                                <a href="/subjects/<%= subjectList.SubjectStudent.dataValues.id %>/give-score">Give Score</a>
                            </td>
                            <td> <%= scoreName[i]%> </td> <% i = i +1 %>
                            <%} else if (subjectList.SubjectStudent.dataValues.score) {%>
                                <%= subjectList.SubjectStudent.dataValues.score %>
                            </td>
                            <td> <%= scoreName[i]%> </td> <% i = i +1 %>
                            <% } %>
                        <% } %>
                </tr> 
            <% } %>
        </tbody>
</table>



</body>
</html>